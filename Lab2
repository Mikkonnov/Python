import time
print (time.ctime())
start = time.time()
f = open ("/Users/mihail_konnovlist.ru/Desktop/Lab AISD/numbers.txt", "r") # Открываем и читаем файл
cnt = 0 # Заводим счётчик чисел
divs = [] # Также Объявляем массив, в котором будут храниться числа, разделённые пробелом
for i in f: # Циклом проходим по файлу и перебрасываем числа из файла в массив, при этом каждое число это отдельная строка  
    divs += i.strip().split()
if len(divs) > 0: # Проверяем заполнен ли файл
    print ("Такие числа")
    for j in divs: # Теперь ищем числа удовлетворяющие условию задачи
        if j.isnumeric() == True: # Проверяем, чтобы в файле числа состояли только из цифр
            if len(j) == len(set(list(j))):
                cnt += 1
                print (j)
    if cnt == 0:
        print ("отсутствуют")
    else:
        print ("Таких чисел:", cnt)
else:
    print("Данный файл пуст")
print ("Время работы алгоритма =", time.time()-start, "секунд")
